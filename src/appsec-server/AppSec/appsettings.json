{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Debug"
        }
    },

    "AllowedHosts": "*",
    "Authentication": {
        "Schemes": {
            "Bearer": {
                "ValidAudiences": [
                    "http://localhost:8401",
                    "https://localhost:44308",
                    "http://localhost:5182",
                    "https://localhost:7076"
                ],
                "ValidIssuer": "dotnet-user-jwts"
            }
        }
    },
    "paths": {
        "repository": "../../../.repository",
        "database": "../../../.database"
    },
    "ENCRYPTION_KEY": null,
    "ConnectionStrings": {
        "DefaultConnection": "mongodb://appsec:appsec@mongo:27017"
    },
    "zap": {
        "url": "http://host.docker.internal:8090",
        "api_key": "123",
        "period": "60"
    },
    "sonar": {
        "url": "http://host.docker.internal:9000",
        "token": "admin:sonar"
    },
    "docker": {
        "tcp": "tcp://host.docker.internal:2375",
        "socket": "unix:///var/run/docker.sock"
    },
    "external_access": {
        "http": "http://host.docker.internal:5080",
        "https": "https://host.docker.internal:5081"
    },
    "ElasticApm": {
        "ServiceName": "AppSecServer",
        "SecretToken": "secret_token_apm",
        "ServerUrl": "url_server_elastic",
        "Environment": "my-environment",
        "CaptureBody": "all",
        "CaptureHeaders": "true",
        "ElasticApm:LogLevel": "debug"
    },
    "ElasticsearchSettings": {
        "NodeUris": "https://320931ab92694659b60593066e37cec1.southamerica-east1.gcp.elastic-cloud.com:443",
        "ApiKey": "api_key_elastic",
        "CloudId": "cloud_id",
        "Index": "appsecserver-{0:yyyy.MM.dd}"
    },
    "Server": {
        "Version": "1.0.0",
        "ADMIN_HASH": "",
        "ENCRYPTION_KEY": ""
    }
}
