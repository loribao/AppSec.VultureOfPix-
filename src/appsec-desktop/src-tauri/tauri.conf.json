{
    "productName": "appsec-client",
    "version": "0.0.1",
    "identifier": "com.vultureofpix.dev",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "plugins": {
        "http": null
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "title": "App Sec",
                "width": 600,
                "height": 600
            }
        ],
        "trayIcon": {
            "iconPath": "icons\\icon.ico",
            "title": "App Sec",
            "tooltip": "App Sec",
            "id": "appsec-tray",
            "menuOnLeftClick": true,
            "iconAsTemplate": false
        },
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
